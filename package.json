{
  "name": "pandocs",
  "displayName": "Pandocs",
  "description": "Read documents from URLs directly in VSCode",
  "version": "0.0.1",
  "publisher": "raymondlor",
  "repository": {
    "type": "git",
    "url": "https://github.com/rolniuq/pandocs"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:pandocs.openUrl"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pandocs.openUrl",
        "title": "Pandocs: Open URL"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/turndown": "^5.0.6",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.3.0",
    "turndown": "^7.1.2"
  }
}
